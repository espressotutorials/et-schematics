{
  "name": "@espressotutorialsgmbh/et-schematics",
  "version": "0.0.7",
  "description": "Some custom angular schematics for Espresso Tutorials projects.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json -w",
    "test": "npm run build && jasmine src/**/*.test.js",
    "release": "release-it"
  },
  "keywords": [
    "schematics"
  ],
  "author": "info@fabianbentz.de",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "ngPackage": {
    "dest": "dist",
    "lib": {
      "entryFile": "src/public_api.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/espressotutorials/et-schematics.git"
  },
  "bugs": {
    "url": "https://github.com/espressotutorials/et-schematics/issues"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "devDependencies": {
    "@angular-devkit/core": "^19.2.8",
    "@angular-devkit/schematics": "^19.2.8",
    "@angular/cdk": "^19.2.10",
    "@angular/compiler": "^19.2.7",
    "@angular/compiler-cli": "^18.0.2",
    "@angular/core": "^19.2.7",
    "@schematics/angular": "^19.2.8",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "jasmine": "^4.0.0",
    "ng-packagr": "^19.2.2",
    "release-it": "^18.1.2",
    "typescript": "^5.6.3"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
